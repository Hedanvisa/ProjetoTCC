<%= form_for ([:admin, trabalho]) do |t| %>
    <div class="panel-body">
        <form role="form">
            <div class="form-group">                            
                
                <%= select_tag 'orientador', options_for_select(@professores.collect{ |p| [p.nome, p.email] }) %>
            
                <%= select_tag 'banca_1', options_for_select(@professores.collect{ |p| [p.nome, p.email] }) %>
            
                <%= select_tag 'banca_2', options_for_select(@professores.collect{ |p| [p.nome, p.email] }) %>

            </div>
        </form>
        <button type="button" class="btn btn-info">
            <%= link_to "Novo Professor", admin_professor_index_path %>
            <span class="glyphicon glyphicon-user"></span>
        </button>
        <button type="button" class="btn btn-info">
            <%= link_to "Editar", admin_professor_index_path %>
            <span class="glyphicon glyphicon-edit"></span> 
        </button>
        
        <%= t.submit "Enviar para Avaliacao", class: 'btn btn-primary pull-right' %>
    </div>
<% end %>